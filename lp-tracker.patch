+    </div>
+  );
+};
+
+// Skeleton Position Card
+const SkeletonCard: React.FC<SkeletonCardProps> = ({ delay = 0, className = '' }) => {
+  return (
+    <div 
+      className={`
+        rounded-lg sm:rounded-xl p-4 sm:p-5 lg:p-6 
