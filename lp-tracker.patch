+  'uniswap-v3-polygon': {
+    baseUrl: 'https://app.uniswap.org',
+    supportedChains: ['polygon'],
+    urlTemplate: '#/pool/{positionId}',
+    requiresPositionId: true,
+  },
+  'uniswap-v3-base': {
