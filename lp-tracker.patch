+    
+    <div className="relative z-10">
+      <div className="h-5 w-32 sm:h-6 sm:w-48 bg-white/20 rounded mb-3 sm:mb-4 animate-pulse" />
+      
+      <div className="flex items-center justify-center" style={{ height: height - 60 }}>
