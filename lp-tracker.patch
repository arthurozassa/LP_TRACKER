+};
+
+// Loading State Indicator
+const LoadingStateIndicator: React.FC<LoadingStateIndicatorProps> = ({ 
+  state, 
+  protocol 
+}) => {
+  const getStateText = () => {
+    switch (state) {
+      case 'scanning':
