+  if (amount >= 1000000) {
+    return `${(amount / 1000000).toFixed(2)}M ${symbol}`;
+  } else if (amount >= 1000) {
+    return `${(amount / 1000).toFixed(2)}K ${symbol}`;
+  } else {
+    return `${amount.toFixed(4)} ${symbol}`;
+  }
+}
