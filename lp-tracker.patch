+
+  const getStateIcon = () => {
+    switch (state) {
+      case 'scanning':
+        return 'üîç';
+      case 'fetching':
+        return 'üìä';
+      case 'calculating':
+        return 'üßÆ';
+      default:
+        return '‚è≥';
+    }
+  };
+
+  return (
+    <div className="flex flex-col sm:flex-row items-center justify-center space-y-2 sm:space-y-0 sm:space-x-2 py-3 sm:py-4">
