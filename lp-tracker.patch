+          amount: data.token1?.amount || 0,
+          decimals: data.token1?.decimals || 18,
+        },
+      },
+      createdAt: data.createdAt || now,
+      updatedAt: now,
+      manageUrl: data.manageUrl || this.protocol.manageUrl,
+    };
+  }
+}
